<form action="/shelters" method="post" id="shelter_form">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <h1>Create a new Shelter</h1>
  
  <label for="name">Name</label>
  <input type="text" name="shelter[name]" id="name"/></br> 

  <label for="address">Address</label>
  <input type="text" name="shelter[address]" id="address"/></br> 

  <label for="city">City</label>
  <input type="text" name="shelter[city]" id="city"/></br> 

  <label for="state">State</label>
  <input type="text" name="shelter[state]" id="state"/></br> 
  
  <label for="zip">zip</label>
  <input type="text" name="shelter[zip]" id="zip"/></br> 
</form>

<button type="submit" form="shelter_form" value="Submit">Create Shelter</button>
